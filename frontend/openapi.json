    "/api/v1/flashcards/practice-sessions/{practice_session_id}/cards": {
      "get": {
        "tags": ["flashcards"],
        "summary": "List Practice Cards",
        "description": "List practice cards for a session, optionally filtering and ordering.",
        "operationId": "flashcards-list_practice_cards",
        "security": [{ "OAuth2PasswordBearer": [] }],
        "parameters": [
          {
            "name": "practice_session_id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "string",
              "format": "uuid",
              "title": "Practice Session Id"
            }
          },
          {
            "name": "status",
            "in": "query",
            "required": false,
            "schema": {
              "anyOf": [
                { "enum": ["pending", "completed", "all"], "type": "string" },
                { "type": "null" }
              ],
              "title": "Status"
            }
          },
          {
            "name": "limit",
            "in": "query",
            "required": false,
            "schema": { "type": "integer", "default": 100, "title": "Limit" }
          },
          {
            "name": "order",
            "in": "query",
            "required": false,
            "schema": {
              "anyOf": [
                { "enum": ["asc", "desc", "random"], "type": "string" },
                { "type": "null" }
              ],
